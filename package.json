{
	"name": "restexpress",
	"version": "1.0.0",
	"description": "restful express generator that likes trains",
	"main": "lib/server",
	"author": "Tayfun Guelcan",
	"license": "MIT",
	"scripts": {
		"dev": "nodemon --exec babel-node src/server",
		"lint": "eslint src",
		"lint:fix": "eslint --fix src",
		"build": "babel src -d lib",
		"heroku-postbuild": "npm run build",
		"start": "node .",
		"serve": "node dist/server",
		"test": "jest --maxWorkers=1 --detectOpenHandles",
		"test:coverage": "yarn run test --coverage",
		"generate": "plop"
	},
	"devDependencies": {
		"@babel/cli": "^7.10.1",
		"@babel/core": "^7.10.2",
		"@babel/node": "^7.10.1",
		"@babel/plugin-proposal-export-default-from": "^7.10.1",
		"@babel/plugin-transform-modules-commonjs": "^7.10.1",
		"@babel/plugin-transform-parameters": "^7.10.1",
		"@babel/preset-env": "^7.10.2",
		"babel-plugin-root-import": "^6.5.0",
		"eslint": "^7.1.0",
		"jsdoc-route-plugin": "^0.1.0",
		"mongodb-memory-server": "^6.6.1",
		"nodemon": "^2.0.4",
		"plop": "^2.7.1",
		"supertest": "^4.0.2"
	},
	"dependencies": {
		"@sendgrid/mail": "^7.2.0",
		"argon2": "^0.26.2",
		"babel-eslint": "^10.1.0",
		"babel-jest": "^26.0.1",
		"body-parser": "^1.19.0",
		"bodymen": "^1.1.1",
		"compression": "^1.7.4",
		"cors": "^2.8.5",
		"dotenv": "^8.2.0",
		"eslint-plugin-jest": "^23.13.2",
		"express": "^4.17.1",
		"express-acl": "^2.0.8",
		"express-device": "^0.4.2",
		"express-jwt": "^5.3.3",
		"express-rate-limit": "^5.1.3",
		"helmet": "^3.22.0",
		"http-status-codes": "^1.4.0",
		"i18n": "^0.10.0",
		"jest": "^26.0.1",
		"jsdoc": "^3.6.4",
		"jsonwebtoken": "^8.5.1",
		"lodash": "^4.17.15",
		"mongoose": "^5.9.16",
		"mongoose-keywords": "^0.4.1",
		"mongoose-to-swagger": "^1.0.3",
		"morgan": "^1.10.0",
		"querymen": "^2.1.4",
		"rand-token": "^1.0.1",
		"request": "^2.88.2",
		"request-promise": "^4.2.5",
		"swagger-jsdoc": "^4.0.0",
		"swagger-ui-express": "^4.1.4"
	},
	"jest": {
		"testEnvironment": "node",
		"setupFilesAfterEnv": [
			"<rootDir>/test/setup.js"
		],
		"testPathIgnorePatterns": [
			"<rootDir>/lib/",
			"<rootDir>/node_modules/"
		],
		"coverageReporters": [
			"json-summary",
			"text",
			"lcov"
		],
		"collectCoverageFrom": [
			"<rootDir>/src/**/*.{js,jsx}",
			"!<rootDir>/node_modules/",
			"!<rootDir>/path/to/dir/"
		]
	}
}
